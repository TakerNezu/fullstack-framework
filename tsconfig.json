{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "$prisma/*": ["./prisma/node_modules/.prisma/*"],
      "$mock/*": ["./mock/*"],
      "$aspida/*": ["./aspida/*"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react"
  }
}
